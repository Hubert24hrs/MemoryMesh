    "@config-plugins/detox": "^7.0.0",
    
    "typescript": "^5.3.0",
    "supabase": "^1.142.0",
    "eas-cli": "^7.8.0",
    
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    
    "@babel/plugin-transform-export-namespace-from": "^7.23.0",
    "@babel/plugin-transform-nullish-coalescing-operator": "^7.23.0",
    "@babel/plugin-transform-optional-chaining": "^7.23.0",
    
    "react-native-dotenv": "^3.4.0",
    "babel-plugin-module-resolver": "^5.0.0"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run type-check && npm test"
    }
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg)"
    ],
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!src/**/*.d.ts",
      "!src/**/index.ts"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "private": true,
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "expo": {
    "autolinking": {
      "exclude": []
    }
  }
}
